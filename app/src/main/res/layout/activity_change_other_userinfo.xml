<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_f4f4f4"
    tools:context="com.yunyisheng.app.yunys.userset.activity.MyInformationActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_49"
            android:background="@color/white">

            <ImageView
                android:id="@+id/img_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:background="@mipmap/blackback" />

            <TextView
                android:id="@+id/te_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="@string/changeziliao"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_16" />

            <TextView
                android:id="@+id/te_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/distance_10"
                android:text="@string/queren"
                android:textColor="@color/color_666"
                android:textSize="@dimen/distance_14" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed_username"
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_48"
            android:layout_marginTop="@dimen/distance_10"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/distance_10"
                android:text="@string/user_name"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_14" />

            <EditText
                android:id="@+id/te_username"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img1"
                android:background="@null"
                android:hint="@string/putname"
                android:gravity="right"
                android:textColor="@color/color_999"
                android:textColorHint="@color/color_999"
                android:textSize="@dimen/distance_14" />

            <ImageView
                android:id="@+id/img1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/blackback_r" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed_userphone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_48"
            android:layout_marginTop="@dimen/distance_1"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/distance_10"
                android:text="@string/dianhua"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_14" />

            <EditText
                android:id="@+id/te_userphone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img2"
                android:background="@null"
                android:gravity="right"
                android:hint="@string/putphonenum"
                android:textColor="@color/color_999"
                android:textColorHint="@color/color_999"
                android:textSize="@dimen/distance_14" />

            <ImageView
                android:id="@+id/img2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/blackback_r" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_ed_usereamil"
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_48"
            android:layout_marginTop="@dimen/distance_1"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/distance_10"
                android:text="@string/eamil"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_14" />

            <EditText
                android:id="@+id/te_useremail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img3"
                android:hint="@string/puteamil"
                android:gravity="right"
                android:textColor="@color/color_999"
                android:textColorHint="@color/color_999"
                android:textSize="@dimen/distance_14" />

            <ImageView
                android:id="@+id/img3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/blackback_r" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_bumen"
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_48"
            android:visibility="gone"
            android:layout_marginTop="@dimen/distance_1"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/distance_10"
                android:text="@string/bumen"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_14" />

            <TextView
                android:id="@+id/te_userbumen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img4"
                android:textColor="@color/color_999"
                android:textSize="@dimen/distance_14" />

            <ImageView
                android:id="@+id/img4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/blackback_r" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_48"
            android:layout_marginTop="@dimen/distance_1"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/distance_10"
                android:text="@string/zhiwei"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_14" />

            <EditText
                android:id="@+id/te_userzhiwei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img5"
                android:background="@null"
                android:hint="@string/putzhiwei"
                android:gravity="right"
                android:textColor="@color/color_999"
                android:textColorHint="@color/color_999"
                android:textSize="@dimen/distance_14" />

            <ImageView
                android:id="@+id/img5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/blackback_r" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_role"
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_48"
            android:visibility="gone"
            android:layout_marginTop="@dimen/distance_1"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/distance_10"
                android:text="@string/jiaose"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_14" />

            <TextView
                android:id="@+id/te_userjiaose"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img6"
                android:textColor="@color/color_999"
                android:textSize="@dimen/distance_14" />

            <ImageView
                android:id="@+id/img6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@mipmap/blackback_r" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_48"
            android:layout_marginTop="@dimen/distance_10"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/distance_10"
                android:text="@string/isshownum"
                android:textColor="@color/color_333"
                android:textSize="@dimen/distance_14" />

            <CheckBox
                android:id="@+id/cb_isshowphone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:button="@null"
                android:background="@drawable/btn_schedule_set_select"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/distance_10"
                android:checked="true"
                />
        </RelativeLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
